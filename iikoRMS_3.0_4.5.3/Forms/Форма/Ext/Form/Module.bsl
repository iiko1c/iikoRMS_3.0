					изм.НазваниеАйко = код;
					изм.Значение = измерение;
				КонецЕсли;
				хб4.Следующая();
			КонецЦикла;	
			хб4.ЗакрытьФайл();
		КонецЕсли;
	КонецЕсли;
	
