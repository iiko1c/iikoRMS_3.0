Функция ИзСтрокиВСпис(Стрр)
	
	Списс = Новый Массив; 
	Стрр2 = СтрЗаменить(Стрр,";",Символы.ПС);
	
	Для ии = 1  по СтрЧислоСтрок(Стрр2) цикл
		Списс.Добавить(СтрПолучитьСтроку(Стрр2,ии));
