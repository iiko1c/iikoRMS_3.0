	КонецЕсли;
	Если не ЗначениеЗаполнено(Объект.ВППрефикс) тогда		
		Объект.ВППрефикс = "!А";
	КонецЕсли;
	Если не ЗначениеЗаполнено(Объект.АПерараоткиПрефикс) тогда		
		Объект.АПерараоткиПрефикс = "!АП";
