		Объект.ДеньАС_ОПЗСПрефикс = "!С";
	КонецЕсли; 
	Если Объект.НеИспользоватьПрефикс тогда 
		Объект.ПНПрефикс = "";
		Объект.АР_ООРППрефикс = "";
		Объект.АР_ОПЗСПрефикс = "";
		Объект.АС_СТПрефикс = "";
		Объект.АС_ОПЗСПрефикс = "";
