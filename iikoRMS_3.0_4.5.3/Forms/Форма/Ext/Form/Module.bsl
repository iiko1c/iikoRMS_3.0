&НаСервере
Функция ОчиститьТЗ(ТЗ, Концепция)
	
	Строка = ТЗ.Количество()-1;
	
	Пока Строка >= 0 цикл		 
		Если СокрЛП(ТЗ[Строка].КонцепцияНазвание) <> Концепция тогда 
			ТЗ.Удалить(Строка);
			Строка = Строка-1;
		Иначе
			Строка = Строка-1;
		КонецЕсли;
	КонецЦикла; 	 
	
	Возврат ТЗ;	
КонецФункции

